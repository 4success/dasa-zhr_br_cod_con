<mvc:View
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:core="sap.ui.core"
        xmlns:layout="sap.ui.layout"
        id="overview"
        controllerName="dasa.custom.controller.Overview">
    <Page title="Termo de Adesão ao Código de Conduta"
          busy="{pageView>/busy}"
          busyIndicatorDelay="{pageView>/delay}">
        <layout:FixFlex id="pdfMainContainer" class="fixFlexFixedSize" fixContentSize="100%">
            <layout:fixContent>
                <core:HTML content="{pdfModel>/content}" />
            </layout:fixContent>
        </layout:FixFlex>
    </Page>
</mvc:View>